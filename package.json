{
  "name": "westman",
  "version": "0.0.4",
  "description": "Telegram bot to manage Westman's Confession.",
  "main": "bin/www",
  "scripts": {
    "start": "node bin/www",
    "development": "nodemon bin/www",
    "test": "npm run format:check",
    "format": "prettier --write --check \"**/*.{js,json,md}\"\n",
    "format:check": "prettier --check \"**/*.{js,json,md}\"\n",
    "update": "yarn update"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.5"
  },
  "keywords": [
    "Telegram",
    "Bot",
    "Helper",
    "Assistant"
  ],
  "author": "Genemator Sakhib <genemators@gmail.com> (https://genemator.uz)",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiseldevs/westman.git"
  },
  "bugs": {
    "url": "https://github.com/chiseldevs/westman/issues"
  },
  "homepage": "https://github.com/chiseldevs/westman#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bad-words": "^3.0.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "is-reachable": "^4.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "telegraf": "^3.38.0"
  }
}
